#include<iostream>
#define LL long long

using namespace std;

LL jie(LL n) {
	if (n == 1) {
		return 1;
	}
	return n*jie(n - 1);
}

LL fun(LL n, LL m) {
	if (m == 1) {
		return 1;
	}
	if (n == 1) {
		return m;
	}else {
		return m*fun(n - 1, m - 1);
	}
}

LL C(LL n, LL m) { // n在上
	if (n == 0) {
		return 1;
	}else {
		return fun(n, m) / jie(n);
	}
}
